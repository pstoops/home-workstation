# {{ ansible_managed }}

deb {{ apt_mirror }} buster main non-free contrib
deb-src {{ apt_mirror }} buster main non-free contrib

deb http://security.debian.org/debian-security buster/updates main contrib non-free
deb-src http://security.debian.org/debian-security buster/updates main contrib non-free

# buster-updates, previously known as 'volatile'
deb {{ apt_mirror }} buster-updates main contrib non-free
deb-src {{ apt_mirror }} buster-updates main contrib non-free

# buster-backports, previously on backports.debian.org
deb {{ apt_mirror }} buster-backports main contrib non-free
deb-src {{ apt_mirror }} buster-backports main contrib non-free

